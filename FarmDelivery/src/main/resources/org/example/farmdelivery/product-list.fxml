<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.farmdelivery.DashboardController"
            prefHeight="1200.0" prefWidth="800.0">

    <TableView fx:id="productTable" prefWidth="800" layoutX="10" layoutY="10">
        <columns>
            <TableColumn styleClass="columnAttribute" fx:id="nameColumn" text="Name" prefWidth="200"/>
            <TableColumn styleClass="columnAttribute" fx:id="imageColumn" text="Image" prefWidth="200"/>
            <TableColumn styleClass="columnAttribute" fx:id="priceColumn" text="Price" prefWidth="200"/>
            <TableColumn styleClass="columnAttribute" fx:id="actionColumn" text="Remove" prefWidth="200"/>
        </columns>
    </TableView>


</AnchorPane>
